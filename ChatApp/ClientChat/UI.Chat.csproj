<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWindowsForms>true</UseWindowsForms>
    <RootNamespace>ClientChat</RootNamespace>
    <AssemblyName>ClientChat</AssemblyName>

    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Chat.Shared\Chat.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="FormChat.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="FormChat.Designer.cs">
      <DependentUpon>FormChat.cs</DependentUpon>
    </Compile>
    </ItemGroup>

</Project>